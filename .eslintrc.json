{
  "extends": 
    [
     "standard",
     "eslint:recommended",
     "plugin:react/recommended"
    ],
   "plugins": 
     [
      "react"
    ],
    "settings": {
      "react": {
        "createClass": "createReactClass",                                   
        "pragma": "React",  
        "version": "16.5.2"
    },
     "propWrapperFunctions": [ "forbidExtraProps" ] 
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}